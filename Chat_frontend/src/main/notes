        HttpClient client = HttpClient.newHttpClient();
        HttpRequest request = HttpRequest.newBuilder()
                .uri(URI.create("http://localhost:8080/api/contacts/accepted"))
                .GET()
                .header("Authorization", "Bearer " + TokenManager.getInstance().getToken())
                .build();
        CompletableFuture<String> body = client.sendAsync(request, HttpResponse.BodyHandlers.ofString())
                .thenApply(HttpResponse::body);
        body.thenAcceptAsync(System.out::println);
